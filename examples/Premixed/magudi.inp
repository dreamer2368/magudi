# mixinglayer.inp

# Filenames, etc.
output_prefix = "Premixed"
grid_file = "Premixed.xyz"
initial_condition_file = "Premixed.ic.q"
boundary_condition_file = "bc.dat"
target_state_file = "Premixed.target.q"
target_mollifier_file = "Premixed.target_mollifier.f"
control_mollifier_file = "Premixed.control_mollifier.f"
mean_pressure_file = "Premixed.mean_pressure.f"

# Grid information
nx = 201
ny = 201
xmin_interior = -10
xmax_interior = 10
ymin_interior = -10
ymax_interior = 10
xmin_outer = -14
xmax_outer = 14
ymin_outer = -14
ymax_outer = 14

# Simulation flags
include_viscous_terms = true
use_constant_CFL_mode = false
use_target_state = true
curvilinear_domain = false
compute_time_average = false
use_manual_domain_decomposition = false

# Non-dimensional constants
Reynolds_number = 200.0
Prandtl_number = 0.7
Schmidt_number_1 = 0.7
Schmidt_number_2 = 0.7
viscosity_power_law_exponent = 0.666
bulk_viscosity_ratio = 0.6
ratio_of_specific_heats = 1.4

# Chemistry
number_of_species = 2
number_of_reactions = 1
initial_fuel_mass_fraction = 1.0
initial_oxidizer_mass_fraction = 0.233
initial_mixture_fraction = 0.1
stoichiometric_ratio = 8.0
heat_release = 0.86
Zel_Dovich = 6.0
Damkohler_number = 5000.0

# Discretization scheme
defaults/discretization_scheme = "SBP 4-8"

# Time stepping options
time_step_size = 0.05
number_of_timesteps = 500
report_interval = 1
save_interval = 20

# Artificial dissipation
add_dissipation = true
dissipation_amount = 0.005

# Solution limits
enable_solution_limits = false
minimum_density = 0.01
minimum_temperature = 0.05
maximum_density = 10.
maximum_temperature = 100.
minimum_massFraction = -0.1
maximum_massFraction = 1.1

# Damping
defaults/sponge_amount = 0.2
defaults/viscous_penalty_amount = 0.

# Acoustic source
number_of_acoustic_sources = 1
acoustic_source01/amplitude = 1.
acoustic_source01/frequency = 0.477464829275686
acoustic_source01/x = -3.
acoustic_source01/radius = 2.1213203435596424

# Adjoint optimization flags
disable_adjoint_solver = false
baseline_prediction_available = false
use_continuous_adjoint = false
cost_functional_type = "SOUND"
gradient_buffer_size = 20

# Gradient accuracy options
check_gradient_accuracy = true
number_of_control_iterations = 32
initial_actuation_amount = 1e4
restart_control_iteration = 0
actuation_amount_geometric_growth = 0.5623413251903491
